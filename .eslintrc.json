{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "quotes": ["warn", "double"],
      "semi": ["warn", "always"],
      "camelcase": ["warn", {
          "properties": "always"
      }],
      "eqeqeq": ["warn", "always"],
      "prefer-const": "warn",
      "indent": "warn"
  }
}